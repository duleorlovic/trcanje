<!DOCTYPE html>
<html>
<head>
  <title>Trcanje <%= full_title(yield(:title)) %></title>
  <!--[if lt IE 9]>
  <script src="http://html5shiv.googlecode.com/svn/trunk/html5.js"></script>
  <![endif]--> 
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>
  <div class="container">
    <header>
      <%= render 'layouts/header' %>
    </header>
    <section class="round">
      <%= yield %>
    </section>
      <%= render 'layouts/footer' %>
      <%= debug(params)  %>
  </div>

</body>
</html>
